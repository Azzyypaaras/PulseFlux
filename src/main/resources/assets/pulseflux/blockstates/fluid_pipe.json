{
  "multipart": [
    { "when": { "north": "false", "south": "false", "east" : "false", "west" : "false", "up" : "false", "down": "false"},
      "apply": { "model": "pulseflux:block/wooden_pipe_core" }
    },
    { "when": { "north": "true", "south": "true", "east" : "false", "west" : "false", "up" : "false", "down": "false"},
      "apply": { "model": "pulseflux:block/wooden_pipe_straight" }
    },
    { "when": { "north": "false", "south": "false", "east" : "true", "west" : "true", "up" : "false", "down": "false"},
      "apply": { "model": "pulseflux:block/wooden_pipe_straight", "y" : 90 }
    },
    { "when": { "north": "false", "south": "false", "east" : "false", "west" : "false", "up" : "true", "down": "true"},
      "apply": { "model": "pulseflux:block/wooden_pipe_straight", "x" : 90 }
    },
    {   "when": { "north": "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm" }
    },
    {   "when": { "south" : "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm", "y": 180 }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm", "y": 90 }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm", "y": 270 }
    },
    {   "when": { "up": "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm", "x": 270 }
    },
    {   "when": { "down": "true" },
      "apply": { "model": "pulseflux:block/wooden_pipe_arm", "x": 90 }
    }
  ]
}